<tr>
  <td><%= user.id %></td>
  <td><%= user.first_name %> <%= user.last_name %></td>
  <td><%= user.email %></td>
  <td><%= user.organization %></td>
  <td><% user.roles.all.each do |role| %> <span><%= role.name %></span><% end %></td>
  <td><%= user.created_at %></td>
  <td><%= link_to "Edit", edit_admin_user_path(user), class: "btn" %>
  <%= link_to "Delete", admin_user_path(user), class: "btn red", data: { method: :delete, confirm: "Are you sure?" } %></td>

</tr>