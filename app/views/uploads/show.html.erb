<p id="notice"><%= notice %></p>
<p>
  <strong>Title:</strong>
  <%= @upload.title %>
</p>
<% (0...@upload.images.count).each do |image|%>
  <%= image_tag(@upload.thumbnail(image)) %>
<% end %>
<p>
  <strong>Description:</strong>
  <%= @upload.description %>
</p>
<p>
  <strong>Body:</strong>
  <%= @upload.body %>
</p>
<%= link_to 'Edit', edit_upload_path(@upload) %> |
<%= link_to 'Back', uploads_path %>
