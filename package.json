{
  "name": "app",
  "private": true,
  "dependencies": {
    "@hotwired/stimulus": "^3.1.0",
    "@hotwired/turbo-rails": "^7.1.3",
    "@pgrabovets/json-view": "^2.7.1",
    "@popperjs/core": "^2.11.5",
    "@rails/actiontext": "^7.0.3-1",
    "@tailwindcss/forms": "^0.5.2",
    "@uppy/core": "^2.3.1",
    "@uppy/dashboard": "^2.4.0",
    "@uppy/image-editor": "^1.4.0",
    "autoprefixer": "^10.4.7",
    "esbuild": "^0.14.50",
    "esbuild-rails": "^1.0.3",
    "flowbite": "1.4.7",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.1.6",
    "trix": "^2.0.0-beta.0",
    "uppy-activestorage-upload": "https://github.com/puglet5/uppy-activestorage-upload"
  },
  "version": "0.1.0",
  "babel": {
    "presets": []
  },
  "browserslist": [
    "defaults"
  ],
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "build:css:dev": "tailwindcss -i ./app/assets/stylesheets/application.scss -o ./app/assets/builds/application.css",
    "build:css": "postcss ./app/assets/builds/application.css -o ./app/assets/builds/application.min.css",
    "build:dev": "node esbuild.config.js",
    "build": "esbuild ./app/assets/builds/application.js --minify > ./app/assets/builds/application.min.js"
  },
  "devDependencies": {
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "cssnano": "^5.1.12",
    "postcss-nested": "^5.0.6"
  }
}
